package firstDecPackage;

public class Static2 {
	int roll;
	String name;
	static String collage = "IIT Mumbai";
	
	public Static2(int roll, String name) {
		this.roll = roll;
		this.name = name;
		System.out.println("Object is created");
	}
	public static void main(String[] args) {
		Static2 s1 = new Static2(007,"Anup");
		Static2 s2 = new Static2(45, "iit ngp");
		System.out.println(s1.roll + s2.name + s2.collage);
				s1.name = "Avish";
				System.out.println(s1.roll + s1.name);
	}
	
}
