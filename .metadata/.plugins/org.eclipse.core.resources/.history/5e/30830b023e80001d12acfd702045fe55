package firstDecPackage;

public class Unary1 {

	public static void main(String[] args) {
		int i=10;  int j=10;
		System.out.println(i--);
		System.out.println(i-- + --i);
		System.out.println(i + i--);
		System.out.println(j-- + j++);
		System.out.println(j-- + j++);
		System.out.println(j);

	}

}
