package firstDecPackage;

import java.util.Scanner;

public class StarConsole {
	public static void main(String args[]){
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int count=0;
		for (int i = 1; i<=n; i++) {
			count = 0;
			for (int j = 0; j <=i; j++) {
			if(n%i==0) {
				count=count+1;
			}
			if(count==2) {
				System.out.println("its an prime number"+i);
			}
			}
		}
		 
	 
}
}	


