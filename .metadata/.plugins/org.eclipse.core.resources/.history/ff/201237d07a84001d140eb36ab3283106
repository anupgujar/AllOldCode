package firstDecPackage;

public class ForLoop {

	public static void main(String[] args) {
		String Colour[] = {"Red","Black","Blue","Green","White","Orange","Pink","Brown","Purple","Marron","Grey","Skyblue"};
		/*
		 * for(int i=1; i<=10; i++) { System.out.println("Print Colour as "+ Colour[i]);
		 * }
		 */
		 
		while(int i<=10) {
			System.out.println("Print Colour as "+ Colour[i]);
			i++;
		}
	}

}
