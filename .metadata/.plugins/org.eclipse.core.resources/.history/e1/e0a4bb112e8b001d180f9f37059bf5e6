package firstDecPackage;

import java.util.Scanner;

public class NextedForLoop {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		for (int i = n; i >=1; i--) {
			for (int j = 1; j <= i; j++) {
				System.out.print("*");
			}System.out.println();	
		}
		int m = 4; 
		 for (int i = 0; i <= m; i++) {
			for (int j = 1; j <=i; j++) {
				int sum =0;
				sum = j+1;
				System.out.print(j);
			}System.out.println();
		}
	} 
}
