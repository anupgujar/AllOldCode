package firstDecPackage;
import java.util.Scanner;
public class Test {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String a = sc.nextLine();
		int b = a.length();
		String rev = "";
		for (int i = a.length()-1; i>=0 ; i--) {
			rev = rev + a.charAt(i);
		}System.out.println(rev);
	}

}
