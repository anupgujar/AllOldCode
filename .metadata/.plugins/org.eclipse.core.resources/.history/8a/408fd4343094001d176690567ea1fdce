package firstDecPackage;

import java.util.Scanner;

public class StarConsole {
	public static void main(String args[]){
		Scanner sc = new Scanner(System.in);
		int n =7;
		int count=0;
		for (int i = 2; i <=n; i++) {
			if(n%i==0) {
				count=count+1;
			}
			if(count==2) {
				System.out.println("its an prime number"+n);
			}
		}
		 
	 
}
}	


