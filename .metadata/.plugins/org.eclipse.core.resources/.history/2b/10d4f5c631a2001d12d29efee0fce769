package firstDecPackage;
public class Test {
	String name;

	public Test(String name) {
		super();
		this.name = name;
	}
	 public static void main(String[] args) {
		 Test cpp = new Test("Main");
		 Test java = new Test("Java");
		 cpp.setName("C++");
		 java.getName();
		 cpp.getName();
		 }
		 void setName(String t) {
		 name = t;
		 }
		 void getName() {
		 System.out.println("Language name: " + name);
		 }

}
