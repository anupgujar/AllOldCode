package firstDecPackage;

public class ReturnTypeHomeWork2 {
	public static void main(String[] args) {
		double data = rembal(2800.456,1200.8467);
		System.out.println(data);
	}
	public static double rembal(double accountbalance, double withdeawnamount){
		double Bankbalance = accountbalance - withdeawnamount; 
		return rembal;
	}
	
	
